[user]
	name = kebo
	email = kebo2021@qq.com
[core]
	editor = nvim
[merge]
	tool = nvim -d
[alias]
	l = log
	t = tag
	a = add
	ai = add -i
	rb = rebase
	co = checkout
	cm = commit -m
	cp = cherry-pick
	ut = rm -r --cached
	st = status -sb
	new = checkout -b
	amend = commit --amend -m
	unstage = reset HEAD^
	uncommit = reset --soft HEAD^
	contributors = shortlog --summary --numbered
	lo = log --oneline
	last = log -1 HEAD
	logg = log --graph --decorate --all
	dog = log --all --decorate --oneline --graph
