[user]
	name = kebo
	email = kebo2021@qq.com
[color]
	branch = auto
	status = auto
	diff = auto
	log = auto
	grep = auto
	interactive = auto
	showBranch = auto
[merge]
	tool = nvim -d
[alias]
	new = checkout -b
	cm = commit -m
	amend = commit --amend -m
	st = status -sb
	br = branch -v
	fo = fetch origin
	fu = fetch upstream
	po = push origin
	unstage = reset HEAD^
	uncommit = reset --soft HEAD^
	branches = branch -a
	remotes = remote -v
	contributors = shortlog --summary --numbered
	lo = log --oneline
	last = log -1 HEAD
	logg = log --graph --decorate --all
	dog = log --all --decorate --oneline --graph
