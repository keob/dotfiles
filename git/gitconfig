[init]
	defaultBranch = main
[core]
	safecrlf = true
	autocrlf = input
	ignoreCase = false
	pager = less
[pull]
	rebase = false
[push]
	default = simple
	followTags = true
[fetch]
	prune = true
[diff]
	algorithm = patience
[http]
	postBuffer = 524288000
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
