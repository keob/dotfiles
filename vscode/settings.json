{
    "editor.tabSize": 4,
    "editor.fontSize": 16,
    "editor.fontLigatures": false,
    "editor.fontFamily": "Iosevka,Microsoft Yahei UI",
    "editor.lineNumbers": "on",
    "editor.detectIndentation": true,
    "editor.cursorBlinking": "expand",
    "editor.hover.sticky": true,
    "editor.smoothScrolling": true,
    "editor.stickyScroll.enabled": true,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.snippetSuggestions": "inline",
    "editor.suggest.showSnippets": true,
    "editor.suggest.insertMode": "replace",
    "editor.renderWhitespace": "selection",
    "editor.wordWrap": "on",
    "editor.links": true,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.acceptSuggestionOnEnter": "on",
    "editor.parameterHints.enabled": true,
    "editor.minimap.enabled": true,
    "editor.cursorSurroundingLines": 5,
    "editor.minimap.showSlider": "always",
    "editor.minimap.renderCharacters": false,
    "editor.semanticHighlighting.enabled": true,
    "editor.defaultColorDecorators": true,
    "editor.experimental.asyncTokenization": true,
    "editor.tabCompletion": "on",
    "editor.dragAndDrop": false,
    "editor.codeLensFontFamily": "Iosevka,Microsoft Yahei UI",
    "editor.linkedEditing": true,
    "editor.minimap.autohide": true,
    "editor.inlayHints.padding": true,
    "editor.guides.bracketPairs": false,
    "editor.find.addExtraSpaceOnTop": false,
    "editor.suggest.preview": false,
    "editor.suggestSelection": "first",
    "editor.suggest.showStatusBar": true,
    "editor.suggest.shareSuggestSelections": true,
    "editor.inlineSuggest.enabled": true,
    "editor.inlineCompletionsAccessibilityVerbose": true,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
    "editor.quickSuggestions": {
        "other": "on",
        "comments": "on",
        "strings": "on"
    },
    "search.exclude": {
        "**/.git": true,
        "**/.github": false,
        "**/.pnpm": true,
        "**/.vscode": true,
        "**/node_modules": true,
        "**/dist/**": true,
        "**/logs": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/package-lock.json": true,
        "**/pnpm-lock.yaml": true,
        "**/yarn.lock": true,
        "**/public": true,
        "**/temp": true,
        "**/CHANGELOG*": true,
        "**/LICENSE*": true,
    },
    "search.followSymlinks": false,
    "files.eol": "\n",
    "files.enableTrash": false,
    "files.insertFinalNewline": true,
    "files.simpleDialog.enable": true,
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/node_modules": true,
    },
    "explorer.compactFolders": false,
    "explorer.excludeGitIgnore": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
    "explorer.fileNesting.patterns": {
        "*.c": "$(capture).h",
        "*.cc": "$(capture).hpp, $(capture).h, $(capture).hxx",
        "*.cxx": "$(capture).hpp, $(capture).h, $(capture).hxx",
        "*.cpp": "$(capture).hpp, $(capture).h, $(capture).hxx",
        "*.css": "$(capture).css.map, $(capture).*.css",
        "*.cjs": "$(capture).cjs.map, $(capture).*.cjs, $(capture)_*.cjs",
        "*.jsx": "${capture}.js, $(capture).*.jsx, $(capture)_*.js, $(capture)_*.jsx",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts, $(capture).*.js, $(capture)_*.js",
        "*.mjs": "$(capture).mjs.map, $(capture).*.mjs, $(capture)_*.mjs",
        "*.ts": "${capture}.js, $(capture).d.ts.map, $(capture).*.ts, $(capture)_*.js, $(capture)_*.ts",
        "*.tsx": "${capture}.ts, $(capture).*.tsx, $(capture)_*.ts, $(capture)_*.tsx",
        "*.vue": "$(capture).*.ts, $(capture).*.js, $(capture).story.vue",
        ".env": "*.env, .env.*, .envrc, env.d.ts",
        ".gitignore": ".gitattributes, .gitmodules, .gitmessage, .mailmap, .git-blame*",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
        // "*.go": "$(capture)_test.go",
        "go.mod": "go.sum",
        "go.work": "go.work.sum",
    },
    "breadcrumbs.icons": true,
    "breadcrumbs.enabled": true,
    "breadcrumbs.filePath": "last",
    "workbench.startupEditor": "none",
    "workbench.list.smoothScrolling": true,
    "workbench.sideBar.location": "left",
    "workbench.editor.enablePreview": true,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Default Dark Modern",
    "workbench.tree.indent": 8,
    "workbench.tree.expandMode": "singleClick",
    "workbench.tree.renderIndentGuides": "onHover",
    "workbench.editor.closeOnFileDelete": true,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.perEditorGroup": true,
    "workbench.editor.limit.value": 6,
    "debug.console.fontSize": 16,
    "debug.onTaskErrors": "debugAnyway",
    "terminal.integrated.shellIntegration.enabled": false,
    "terminal.integrated.fontSize": 16,
    "zenMode.centerLayout": false,
    "zenMode.hideLineNumbers": false,
    "window.zoomLevel": 0,
    "window.dialogStyle": "custom",
    "window.title": "${rootName}",
    "window.titleBarStyle": "custom",
    "update.mode": "manual",
    "diffEditor.ignoreTrimWhitespace": false,
    "extensions.autoUpdate": "onlyEnabledExtensions",
    "extensions.ignoreRecommendations": true,
    "telemetry.telemetryLevel": "off",
    "emmet.triggerExpansionOnTab": true,
    "emmet.showSuggestionsAsSnippets": true,
    "emmet.includeLanguages": {
        "vue-html": "html",
        "vue": "html",
        "razor": "html",
        "plaintext": "pug"
    },
    "go.lintOnSave": "off",
    "go.lintTool": "revive",
    "go.formatTool": "gofmt",
    "gopls": {
        "ui.diagnostic.analyses": {
            "useany": true,
            "nilness": true,
            "unusedwrite": true,
            "unusedparams": true,
            "unusedvariable": true,
        },
        "ui.semanticTokens": false,
        "ui.diagnostic.staticcheck": false,
        "ui.completion.usePlaceholders": true,
        "ui.documentation.linksInHover": false,
    },
    "[go]": {
        "editor.insertSpaces": false,
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "always"
        }
    },
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.untrackedChanges": "separate",
    "scm.diffDecorationsGutterWidth": 2,
    "npm.enableRunFromFolder": true,
    "json.schemaDownload.enable": false,
    "typescript.enablePromptUseWorkspaceTsdk": true,
    "typescript.disableAutomaticTypeAcquisition": true,
    "typescript.updateImportsOnFileMove.enabled": "always"
}
