{
	"Console Log": {
		"prefix": "cl",
		"body": [
			"console.log($0);"
		]
	},
	"Console Error": {
		"prefix": "ce",
		"body": [
			"console.error($0);"
		]
	},
	"Console Clear": {
		"prefix": "cc",
		"body": [
			"console.clear();"
		]
	},
	"Destructure": {
		"prefix": "d",
		"body": [
			"const { $1 } = $0"
		]
	},
	"Try/Catch": {
		"prefix": "try",
		"body": [
			"try {",
			"  $1",
			"} catch (err) {",
			"  $0",
			"}"
		]
	},
	"To-Do": {
		"prefix": "todo",
		"body": [
			"// TODO: $1"
		]
	},
	"Function": {
		"prefix": "f",
		"body": [
			"function $1($2) {",
			"  $0",
			"}"
		]
	},
	"Export Default": {
		"prefix": "ed",
		"body": [
			"export default "
		]
	}
}